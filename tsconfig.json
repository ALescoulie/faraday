{
  "compilerOptions": {
    "incremental": true,
    "target": "es2016",
    "sourceMap": true,
    "module": "ES2015",
    "lib": ["ES5", "ES6", "DOM"],
    "noEmit": true,
    "moduleResolution":"node",
    "allowImportingTsExtensions": true,
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true, 
    "strict": true,                   
    "skipLibCheck": true,
    "jsx": "react"
  }
}
